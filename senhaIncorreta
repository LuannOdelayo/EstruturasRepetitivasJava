import java.util.Scanner;

public class ex3 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int senha = sc.nextInt();

        while (senha != 2000) {
            System.out.println("Senha incorreta");
            senha = sc.nextInt();
        }

        System.out.println("Acesso liberado");
        
        sc.close();
    }
}
